<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Express Dev Skills</title>
</head>
<body>
    <h1>Developer Skills as of <%= time %></h1>
    <ul>
        <% skills.forEach(function(s, idx) { %>
            <li>
                <a href="/skills/<%= idx %>"><%= s.skill %></a>
                 - <%= s.learned ? 'Learned' : 'Not Learned' %>
            </li>
        <% }); %>
    </ul>
    <a href="/skills/new">Add Skill</a>
  </body>
</html>